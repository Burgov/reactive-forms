{
  "name": "reactive-forms-playground",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest --coverage=false && npm run test:types",
    "test:watch": "jest --watch --coverage=false",
    "test:types": "cd projects/ngneat/reactive-forms/src/lib/type-tests && tsc --outDir ./out-tsc",
    "test:lib:headless": "cross-env CI=true npm run test",
    "lint": "ng lint",
    "schematics:watch": "cd ./schematics/ && npm link && cd .. && npm link @ngneat/reactive-forms && npm run build --prefix ./schematics -- --watch",
    "schematics:copy": "cp -r schematics/src/ dist/ngneat/reactive-forms/schematics",
    "schematics:build": "npm run build --prefix ./schematics",
    "contributors:add": "all-contributors add",
    "hooks:pre-commit": "node hooks/pre-commit.js",
    "commit": "git-cz",
    "release:first": "npm run release -- --first-release",
    "build:lib": "ng build @ngneat/reactive-forms --prod && npm run schematics:build && npm run copy",
    "release": "cd projects/ngneat/reactive-forms && standard-version --infile ../../../CHANGELOG.md",
    "copy": "npm run schematics:copy && cp -r README.md dist/ngneat/reactive-forms",
    "lint:playground": "eslint ./src --ext .ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.7",
    "@angular/common": "~10.0.7",
    "@angular/compiler": "~10.0.7",
    "@angular/core": "~10.0.7",
    "@angular/forms": "~10.0.7",
    "@angular/platform-browser": "~10.0.7",
    "@angular/platform-browser-dynamic": "~10.0.7",
    "@angular/router": "~10.0.7",
    "@ngneat/lib": "^1.0.0",
    "requireindex": "^1.1.0",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.5",
    "@angular/cli": "~10.0.5",
    "@angular-devkit/build-ng-packagr": "~0.1000.5",
    "@angular/compiler-cli": "~10.0.7",
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-angular": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@ngneat/spectator": "^4.11.1",
    "@phenomnomnominal/tsquery": "^4.1.0",
    "@types/jest": "25.2.2",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "all-contributors-cli": "^6.8.1",
    "codelyzer": "^5.1.2",
    "cross-env": "^5.2.0",
    "eslint": "^6.8.0",
    "expect-type": "^0.7.4",
    "git-cz": "^3.2.1",
    "husky": "^3.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "26.0.1",
    "jest-preset-angular": "8.2.0",
    "lint-staged": "^9.2.0",
    "mocha": "^3.1.2",
    "ng-packagr": "^10.0.3",
    "prettier": "^1.18.2",
    "protractor": "~5.4.3",
    "standard-version": "^6.0.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-commit": "npm run hooks:pre-commit && lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,ts,html,component.html}": [
      "prettier --write",
      "git add"
    ]
  }
}
